<%- include("partials/header") %>

<h1 class="compose-header">Compose</h1>
<form class="form-g" action="/compose" method="post">
  <div class="form-group">
    <input class="form-control inp inp-h" type="text" name="title" value="" placeholder="Article Title">
    <textarea class="form-control inp" name="content" rows="10" cols="30" placeholder="Write your article"></textarea>
  </div>
  <button class="btn btn-primary btn-lg" type="submit" name="button">Publish</button>
</form>

